{
	"compilerOptions": {
		"module": "commonjs",
		"target": "es2019",

		"strict": true,
		"strictPropertyInitialization": false,
		"esModuleInterop": true,
		"skipLibCheck": true,
		"forceConsistentCasingInFileNames": true,
		"removeComments": true,
		//DO NOT TOUCH UNDER HERE
		"emitDecoratorMetadata": true,
		"experimentalDecorators": true,
		"outDir": "./dist",
		"baseUrl": ".",
		"paths": {
			"src/*": ["./src/*"],
			"@services/*": ["./src/services/*"]
		},
		"typeRoots": ["./src/types/mustache-async.d.ts"]
	},
	"include": ["./src/**/*"],
	"exclude": ["./src/main2.ts"] //Extra file
}
